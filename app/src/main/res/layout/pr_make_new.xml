<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.woojinkim.passiveroutineproto.MainActivity">

    <TextView
        android:id="@+id/pr_make_new_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:text="pr_make_new" />

    <Button
        android:id="@+id/pr_make_new_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="X" />

    <Button
        android:id="@+id/pr_make_new_trigger"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="trigger" />

    <Button
        android:id="@+id/pr_make_new_action"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="action" />
</LinearLayout>